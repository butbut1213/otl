<script type="text/javascript">
	// Parse the URL parameter
	function getParameterByName(name, url) {
	    if (!url) url = window.location.href;
	    name = name.replace(/[\[\]]/g, "\\$&");
	    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
	        results = regex.exec(url);
	    if (!results) return null;
	    if (!results[2]) return '';
	    return decodeURIComponent(results[2].replace(/\+/g, " "));
	}
	// Give the parameter a variable name
	var dynamicContent = getParameterByName('act');
 
	 $(document).ready(function() {
 
		// Check if the URL parameter is apples
		if (dynamicContent == '12') {
			$('#12').show();
		} 
		// Check if the URL parameter is oranges
		else if (dynamicContent == '13') {
			$('#13').show();
		} 
		// Check if the URL parameter is bananas
		else if (dynamicContent == '14') {
			$('#14').show();
		} 
		 else if (dynamicContent == '15') {
			$('#15').show();
		}
		 else if (dynamicContent == '16') {
			$('#16').show();
		}
		  else if (dynamicContent == '17') {
			$('#17').show();
		}
		  else if (dynamicContent == '18') {
			$('#18').show();
		}
		  else if (dynamicContent == '19') {
			$('#19').show();
		}
		 
		// Check if the URL parmeter is empty or not defined, display default content
		else {
			$('#default-content').show();
		}
	});
</script>
	
	
	
	
